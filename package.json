{
  "name": "croquilapp",
  "version": "1.0.0",
  "main": "main.js",
  "description": "Croquil",
  "scripts": {
    "build": "webpack -p",
    "start": "NODE_ENV=dev electron .",
    "package": "NODE_ENV=prod && npm run build && electron-packager ./ Croquil --out ./bin --platform=darwin --arch=x64 --version=0.36.0 --overwrite --ignore=\"ignore|.bin|bin|web/src|web/css|webpack.config.js|.gitignore|.editorconfig|.babelrc|css|cfg|README.md\"",
    "watch": "webpack-dev-server --hot --inline"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/helderfarias/croquilapp.git"
  },
  "keywords": [
    "croquil"
